<odoo>
    <data>

        <template id="assets_threecx" name="threecx assets" inherit_id="web.assets_backend">
            <xpath expr="//link[last()]" position="after">
                <link rel="stylesheet" href="/threecx/static/src/less/threecx.css"/>
            </xpath>

            <xpath expr="//script[last()]" position="after">
                <script type="text/javascript" src="/threecx/static/src/js/threecx.js"></script>
            </xpath>
        </template>
    </data>
</odoo>